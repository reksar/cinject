{

"version": "2.0.0",
"tasks": [
  {
    "label": "MSVC x64 Debug",
    "group": {
      "kind": "build",
      "isDefault": true
    },
    "dependsOn": [
      "test",
      "injector"
    ]
  },
  {
    "label": "test",
    "type": "shell",
    "command": "cl",
    "args": [
      "/MD",
      "/W4",
      "/EHsc",
      "/std:c++17",
      "/O2",
      "/Fobuild\\",
      "/Febuild\\",
      "test.cpp",
    ],
    "options": {
      "shell": {
        "executable": "C:\\Windows\\System32\\cmd.exe",
        "args": [
          "/D",
          "/C",
          "CALL \"C:\\Program Files (x86)\\Microsoft Visual Studio\\2019\\BuildTools\\VC\\Auxiliary\\Build\\vcvars64.bat\"",
          "&&"
        ]
      }
    },
    "presentation": {
      "echo": true,
      "reveal": "silent",
      "focus": false,
      "panel": "shared",
      "showReuseMessage": true,
      "clear": false
    },
  },
  {
    "label": "injector",
    "type": "shell",
    "command": "cl",
    "args": [
      "/Z7",
      "/MDd",
      "/W4",
      "/EHsc",
      "/std:c++17",
      "/Od",
      "/Fobuild\\",
      "/Febuild\\",
      "injector.cpp",
    ],
    "options": {
      "shell": {
        "executable": "C:\\Windows\\System32\\cmd.exe",
        "args": [
          "/D",
          "/C",
          "CALL \"C:\\Program Files (x86)\\Microsoft Visual Studio\\2019\\BuildTools\\VC\\Auxiliary\\Build\\vcvars64.bat\"",
          "&&"
        ]
      }
    },
    "presentation": {
      "echo": true,
      "reveal": "silent",
      "focus": false,
      "panel": "shared",
      "showReuseMessage": true,
      "clear": false
    },
  }
]

}
