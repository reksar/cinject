{

"version": "2.0.0",
"tasks": [
  {
    "label": "Compile MSVC x64",
    "group": {
      "kind": "build",
      "isDefault": true
    },
    "dependsOn": [
      "Compile injector"
    ]
  },
  {
    "label": "Compile test",
    "type": "shell",
    "command": "cl",
    "args": [
      "/MD",
      "/W4",
      "/EHsc",
      "/std:c++17",
      "/O2",
      "/Fobuild\\",
      "/Febuild\\",
      "test.cpp"
    ],
    "options": {
      "shell": {
        "executable": "C:\\Windows\\SysWOW64\\cmd.exe",
        "args": [
          "/D",
          "/C",
          "CALL \"C:\\Program Files (x86)\\Microsoft Visual Studio\\2019\\BuildTools\\VC\\Auxiliary\\Build\\vcvars64.bat\"",
          "&&"
        ]
      }
    },
    "presentation": {
      "echo": false,
      "reveal": "silent",
      "focus": false,
      "panel": "shared",
      "showReuseMessage": true,
      "clear": true
    },
    "problemMatcher": []
  },
  {
    "label": "Compile injector",
    "type": "shell",
    "command": "cl",
    "args": [
      "/Z7",
      "/MDd",
      "/W4",
      "/EHsc",
      "/std:c++17",
      "/Od",
      "/Fobuild\\",
      "/Febuild\\",
      "injector.cpp"
    ],
    "options": {
      "shell": {
        "executable": "C:\\Windows\\SysWOW64\\cmd.exe",
        "args": [
          "/D",
          "/C",
          "CALL \"C:\\Program Files (x86)\\Microsoft Visual Studio\\2019\\BuildTools\\VC\\Auxiliary\\Build\\vcvars64.bat\"",
          "&&"
        ]
      }
    },
    "presentation": {
      "echo": true,
      "reveal": "always",
      "focus": true,
      "panel": "shared",
      "showReuseMessage": true,
      "clear": true
    }
  }
]

